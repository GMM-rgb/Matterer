{
  "compilerOptions": {
    "pretty": true,
    "removeComments": true,
    "declaration": false,
    "diagnostics": true,
    "skipLibCheck": true,
    "outDir": "./build",
    "moduleResolution": "bundler",
    "module": "ESNext",
    "lib": ["ES2015", "DOM"],
    "target": "ES6"
  },
  "include": [
    "main.ts",
    "turbowarp-types/**/*.d.ts"
  ],
  "exclude": [
    "build",
    "turbowarp-types/scratch-gui.d.ts"
  ]
}