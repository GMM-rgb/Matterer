{
  "buildOptions": {
    "verbose": true,
    "traceResolution": true,

  },
  "compilerOptions": {
    "pretty": true,
    "alwaysStrict": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "diagnostics": true,
    "declaration": true,
    "outDir": "./build",
    "moduleResolution": "nodenext",
    "lib": ["ES2015", "DOM"],
    "module": "node20",
    "target": "es6",
   }
}